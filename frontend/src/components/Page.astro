---
type Props = {
  title: string;
};

import Head from "./Head.astro";
const { title } = Astro.props;
import "../styles/index.css";
---

<!DOCTYPE html>
<html lang="en">
  <Head title={title} />
  <body>
    <slot name="navbar" />
    <main class="py-16">
      <slot />
    </main>
    <slot name="footer" />
    <slot name="user-modal" />
    <slot name="pending-results-updater" />
    <slot name="user-storage" />
  </body>
</html>
